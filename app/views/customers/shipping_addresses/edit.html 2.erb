<div class="container">
	<div class="row">
	 <div class="shipping-title">
	   <h3>配送先編集</h3>

	    <%= form_for @shipping_address, url:customers_shipping_address_path(@shipping_address.id) do |f| %>
         <%= f.label :"郵便番号(ハイフンなし)" %>
         <%= f.text_field :postal_code %><br>
         <%= f.label :住所 %>
         <%= f.text_field :address %><br>
         <%= f.label :宛名 %>
         <%= f.text_field :name %>
         <%= f.submit "編集する" %>
       <% end %>

	 </div>
	</div>
</div>