<div class="midasi">
	<h2>注文履一覧</h2>
</div>

<table class="table table-hover table-inverse">
	<thead>
		<tr>
			<th>購入日時</th>
			<th>購入者</th>
			<th>注文数</th>
			<th>注文ステータス</th>
		</tr>
	</thead>

	<tbody>
		<% @order_lists.each do |order_list| %>
		<tr>
			<td><a href="<%= admins_order_list_path(order_list.id) %>"><%= order_list.created_at%></a></td>
			<td><%= order_list.name_sei%><%= order_list.name_mei%></td>
			<td><%= order_list.number%></td>
			<td><%= order_list.status%></td>
		</tr>
		<% end %>
	</tbody>
</table>