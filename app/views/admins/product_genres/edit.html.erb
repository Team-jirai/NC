<div class="midasi">
	<h3>ジャンル編集</h3>
</div>

<div class="genre_form">
<%= form_for @genre, url: admins_product_genre_path(@genre.id) do |f| %>
		<%= f.label :ジャンル %>
		<%= f.text_field :genre %>
		<%= f.radio_button :is_valid, :effectiveness %>
		<%= f.label :is_valid, "有効", {style: "display: inline-block;"} %>
  		<%= f.radio_button :is_valid, :uneffectiveness %>
  		<%= f.label :is_valid, "無効", {style: "display: inline-block;"} %>
  		<%= f.submit '変更' %>
<% end %>
</div>